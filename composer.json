{
    "name": "arnissolle/scaleway",
    "description": "Scaleway API client + CLI",
    "authors": [
        {
            "name": "Pierre Arnissolle",
            "email": "pierre@arnissolle.com"
        }
    ],
    "keywords": [
        "scaleway",
        "api",
        "cli"
    ],
    "homepage": "https://github.com/arnissolle/scaleway",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arnissolle\\Scaleway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnissolle\\Scaleway\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/arnissolle"
        }
    ]
}
